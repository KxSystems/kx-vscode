    "keybindings": [
      {
        "command": "kdb.execute.selectedQuery",
        "key": "ctrl+d",
        "mac": "cmd+d",
        "when": "resourceFilename =~ /\\.(?:q|sql)$/i"
      },
      {
        "command": "kdb.execute.fileQuery",
        "key": "ctrl+shift+d",
        "mac": "cmd+shift+d",
        "when": "resourceFilename =~ /\\.(?:q|sql)$/i"
      },
      {
        "command": "kdb.scratchpad.python.run",
        "key": "ctrl+d",
        "mac": "cmd+d",
        "when": "resourceFilename =~ /\\.(?:py)$/i"
      },
      {
        "command": "kdb.scratchpad.python.run.file",
