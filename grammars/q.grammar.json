{
  "name": "Q",
  "scopeName": "source.q",
  "fileTypes": ["q"],
  "patterns": [
    {
      "name": "comment.line.q",
      "match": "^\\s*//.*$"
    },
    {
      "name": "constant.numeric.q",
      "match": "\\b-?[0-9]+(\\.[0-9]+)?\\b"
    },
    {
      "name": "constant.character.q",
      "match": "'.'"
    },
    {
      "name": "keyword.control.q",
      "match": "\\b(Mlim|Q7|access|accp|adict|arch|assign|badtail|branch|cast|char|conn|constants|domain|elim|glim|globals|length|limit|locals|loop|mismatch|mq|noamend|nosocket|noupdate|nyi|os|par|params|part|pl|pwuid|splay|srv|stack|step|stop|stype|sys|threadview|unmappable|upd|user|utf8|vd1|wha|wsfull|wsm)\\b"
    },
    {
      "name": "keyword.operator.q",
      "match": "\\b(and|or|not|in|like)\\b"
    },
    {
      "name": "storage.type.q",
      "match": "\\b(int|long|float|double|char|symbol|timestamp)\\b"
    },
    {
      "name": "variable.other.q",
      "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
    },
    {
      "name": "punctuation.definition.string.begin.q",
      "match": "\""
    },
    {
      "name": "punctuation.definition.string.end.q",
      "match": "\""
    },
    {
      "name": "punctuation.definition.variable.q",
      "match": "\\$"
    },
    {
      "name": "punctuation.definition.comment.q",
      "begin": "/\\*",
      "end": "\\*/"
    },
    {
      "name": "support.function.q",
      "match": "\\b(a[a-zA-Z_0-9]*|abs|aj|acos|all|and|any|asc|asin|asof|atan|attr|avg|avgs|bin|binr|by|ceiling|cols|cor|cos|count|cov|cross|csv|cut|delete|deltas|desc|dev|differ|distinct|div|do|dsave|each|ej|ema|enlist|eval|except|exec|exit|exp|fby|fills|first|flip|floor|fkeys|from|get|getenv|group|gtime|hclose|hcount|hdel|hopen|hsym|iasc|idesc|if|ij|in|insert|inter|inv|key|keys|last|ljf|like|lj|load|log|lower|lsq|ltime|ltrim|mavg|max|maxs|mcount|md5|mdev|med|meta|min|mins|mmax|mmin|mmu|mod|msum|neg|next|not|null|or|over|parse|peach|pj|prd|prds|prev|prior|rand|rank|ratios|raze|read0|read1|reciprocal|reval|reverse|rload|rotate|rsave|rtrim|save|scan|scov|sdev|svar|select|set|setenv|show|signum|sin|sqrt|ss|ssr|string|sublist|sum|sums|sv|system|tables|tan|til|trim|type|uj|ungroup|union|update|upper|upsert|value|var|view|views|vs|wavg|where|while|within|wj|wsum|ww|xasc|xbar|xcol|xcols|xdesc|xexp|xgroup|xkey|xlog|xprev|xrank)[a-zA-Z_][a-zA-Z0-9_.]*\\("
    }
  ]
}
